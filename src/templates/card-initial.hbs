{{#each this}}

<li class="cards-st-p">
	<a href="" class="cards__link">
		<p style="background-color: red; margin: 0;">1-th child of class="cards"</p>
		<div class="product-card__img-container-st-p">
			<img class="product-card__img" src="{{imageUrls}}" data-sourse="" alt="photo" />
		</div>
		<div class="cards__text-cont">
			<h3 class="name-st-p">{{title}}</h3>
			<div class="price-cont">
				<h3 class="old-price-st-p is-hidden">{{oldPrice}}<span class="UAH"> грн</span></h3>
				<h3 class="price-st-p">{{price}}<span> грн</span></h3>
			</div>
		</div>
		<div class="cards__overlay">
			<button class="heart-button icons-container">
				<span class="material-icons cards__icons">
					favorite_border
				</span>
			</button>
			<button class="create-button icons-container">
				<span class="material-icons cards__icons">
					create
				</span>
			</button>
			<button class="fullscreen-button icons-container">
				<span class="material-icons cards__icons">
					fullscreen
				</span>
			</button>
		</div>
	</a>
</li>

{{/each}}